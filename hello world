//c
printf("Hello World!");

//c++
cout<<"Hello World!";

#python
print('Hello World!')

//Java
System.out.println("Hello World!");

;Assembly language
section .data            
        msg db "Hello, world!", 0xA     
        len equ $ - msg                 
section .text            
global _start            
_start:                  
        mov edx, len     
        mov ecx, msg     
        mov ebx, 1       
        mov eax, 4       
        int 0x80         
                         
        mov ebx, 0       
        mov eax, 1       
        int 0x80         
